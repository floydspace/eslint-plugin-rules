{
  "name": "@floydspace/eslint-plugin-rules",
  "version": "0.2.0",
  "description": "ESLint rules plugin",
  "keywords": [
    "eslint",
    "tslint",
    "eslintplugin",
    "eslint plugin",
    "eslint-plugin",
    "typescript",
    "optional-chain",
    "optional chain",
    "optional-chaining",
    "optional chaining",
    "use-optional-chain",
    "use-optional-chaining",
    "prefer-optional-chain",
    "prefer-optional-chaining"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floydspace/eslint-plugin-rules.git"
  },
  "author": "Victor Korzunin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floydspace/eslint-plugin-rules/issues"
  },
  "homepage": "https://github.com/floydspace/eslint-plugin-rules#readme",
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.11",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "coveralls": "^3.0.8",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tsutils": "^3.17.1",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^5.0.0 || ^6.0.0",
    "typescript": ">=3.7.0"
  }
}
